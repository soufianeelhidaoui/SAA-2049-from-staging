<div class="ui stackable two column grid">
    {% for block in element.blocks %}
        <div class="column">
            <h4>{{ block.title|default('') }}</h4>
            <img class="ui image" src="{{ block.image }}" alt="{{ block.alt|default('') }}" title="{{ block.title|default('') }}" style="width: 150px; height: 150px;">
        </div>
    {% endfor %}
</div>
